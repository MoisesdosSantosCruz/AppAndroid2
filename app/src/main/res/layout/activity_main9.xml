<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_launcher_background"
    android:backgroundTint="@color/lightbeige"
    tools:context=".MainActivity9">

    <TextView
        android:id="@+id/txtHome5"
        android:layout_width="415dp"
        android:layout_height="70dp"
        android:background="@drawable/ic_launcher_background"
        android:backgroundTint="@color/beige"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/Home_titulo"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtSobre"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="36dp"
        android:autoSizeTextType="uniform"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/Araraazul"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgAnima5"
        app:layout_constraintTop_toBottomOf="@+id/txtHome5" />

    <TextView
        android:id="@+id/txtAviso"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="36dp"
        android:autoSizeTextType="uniform"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/Ararinha"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgAnima6"
        app:layout_constraintTop_toBottomOf="@+id/txtSobre" />

    <TextView
        android:id="@+id/txtSobre2"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="36dp"
        android:autoSizeTextType="uniform"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/botocorderosa"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgAnima7"
        app:layout_constraintTop_toBottomOf="@+id/txtAviso" />

    <TextView
        android:id="@+id/txtSobre4"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="36dp"
        android:autoSizeTextType="uniform"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/Mitoleaodourado"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgAnima8"
        app:layout_constraintTop_toBottomOf="@+id/txtSobre2" />

    <Button
        android:id="@+id/btnAnima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="54dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="136dp"
        android:backgroundTint="@color/black"
        android:contentDescription="@string/Aviso"
        android:gravity="center_vertical"
        android:onClick="linkMain7"
        android:text="@string/btnlink4"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/btnAnima2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgAnima8"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btnAnima2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="44dp"
        android:backgroundTint="@color/black"
        android:gravity="center_vertical"
        android:text="@string/Home"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:onClick="linkMain1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnAnima"
        app:layout_constraintTop_toBottomOf="@+id/txtSobre4"
        tools:ignore="TouchTargetSizeCheck" />

    <View
        android:id="@+id/viewborder4"
        android:layout_width="415dp"
        android:layout_height="100dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="-70dp"
        android:background="@drawable/ic_launcher_background"
        android:backgroundTint="@color/beige"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imgAnima5"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="37dp"
        android:contentDescription="@string/Araraazul"
        app:layout_constraintEnd_toStartOf="@+id/txtSobre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHome5"
        app:srcCompat="@drawable/animal5" />

    <ImageView
        android:id="@+id/imgAnima6"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="42dp"
        android:contentDescription="@string/Ararinha"
        app:layout_constraintEnd_toStartOf="@+id/txtAviso"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgAnima5"
        app:srcCompat="@drawable/animal6" />

    <ImageView
        android:id="@+id/imgAnima7"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="42dp"
        android:contentDescription="@string/botocorderosa"
        app:layout_constraintEnd_toStartOf="@+id/txtSobre2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgAnima6"
        app:srcCompat="@drawable/animal7" />

    <ImageView
        android:id="@+id/imgAnima8"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="42dp"
        android:contentDescription="@string/Mitoleaodourado"
        app:layout_constraintEnd_toStartOf="@+id/txtSobre4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgAnima7"
        app:srcCompat="@drawable/animal8" />

</androidx.constraintlayout.widget.ConstraintLayout>